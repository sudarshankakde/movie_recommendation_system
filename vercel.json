{
    "functions": {
        "api": {
            "runtime": "python3.9",
            "memory": 512
        }
    },
    "builds": [{
        "src": "movie_recommendation_system/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb" , "src"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "movie_recommendation_system/wsgi.py"
        }
    ]
}